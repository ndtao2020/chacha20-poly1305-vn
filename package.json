{
    "name": "chacha-poly-wasm-web",
    "type": "module",
    "collaborators": [
        "ndtao2020"
    ],
    "description": "ChaCha20-Poly1305 encryption for WebAssembly",
    "version": "1.3.1",
    "license": "MIT License",
    "repository": {
        "type": "git",
        "url": "https://github.com/ndtao2020/chacha20-poly1305-vn"
    },
    "files": [
        "web/",
        "deno/",
        "nodejs/",
        "bundler/"
    ],
    "main": "./web/chacha_poly_wasm_web.js",
    "types": "./web/chacha_poly_wasm_web.d.ts",
    "sideEffects": [
        "./snippets/*"
    ],
    "exports": {
        "./bundler": {
            "types": "./bundler/chacha_poly_wasm_web.d.ts",
            "require": "./bundler/chacha_poly_wasm_web.js",
            "import": "./bundler/chacha_poly_wasm_web.js"
        },
        "./deno": {
            "types": "./deno/chacha_poly_wasm_web.d.ts",
            "require": "./deno/chacha_poly_wasm_web.js",
            "import": "./deno/chacha_poly_wasm_web.js"
        },
        "./nodejs": {
            "types": "./nodejs/chacha_poly_wasm_web.d.ts",
            "require": "./nodejs/chacha_poly_wasm_web.js",
            "import": "./nodejs/chacha_poly_wasm_web.js"
        },
        "./web": {
            "types": "./web/chacha_poly_wasm_web.d.ts",
            "require": "./web/chacha_poly_wasm_web.js",
            "import": "./web/chacha_poly_wasm_web.js"
        },
        ".": {
            "types": "./web/chacha_poly_wasm_web.d.ts",
            "require": "./web/chacha_poly_wasm_web.js",
            "import": "./web/chacha_poly_wasm_web.js"
        }
    },
    "keywords": [
        "chacha20",
        "poly1305",
        "encryption",
        "wasm",
        "webassembly",
        "crypto",
        "vn"
    ]
}